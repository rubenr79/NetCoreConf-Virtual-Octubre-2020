@model string[]
@{
    ViewData["Title"] = "Get Teams from protected API";
}

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-striped table-condensed" style="font-family: monospace">
    <tr>
        <th>Team</th>
        <th>Value</th>
    </tr>
    @{
        foreach (var team in Model)
        {
            <tr>
                <td> @team </td>
            </tr>
        }
    }
</table>
